{
    "name": "overthink/laravel-push-notifications",
    "description": "Send push notifications to Android and iOS",
    "version": "1.0.0",
    "keywords": [
        "laravel",
        "fcm",
        "apns",
        "google push",
        "apple push"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Marko"
        }
    ],
    "support": {
        "email": "marko.zagar@gmail.com"
